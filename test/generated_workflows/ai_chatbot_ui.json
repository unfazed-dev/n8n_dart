{
  "name": "AI Chatbot with UI",
  "active": false,
  "version": 1.0,
  "nodes": [
    {
      "id": "chat_trigger",
      "name": "Chat Trigger",
      "type": "n8n-nodes-langchain.chatTrigger",
      "typeVersion": 1,
      "position": [
        0.0,
        0.0
      ],
      "parameters": {
        "mode": "hostedChat",
        "options": {
          "title": "AI Assistant",
          "subtitle": "Ask me anything!",
          "inputPlaceholder": "Type your message..."
        }
      }
    },
    {
      "id": "ai_agent",
      "name": "AI Agent",
      "type": "n8n-nodes-langchain.agent",
      "typeVersion": 1,
      "position": [
        300.0,
        0.0
      ],
      "parameters": {
        "agent": "conversationalAgent",
        "promptType": "define",
        "text": "You are a helpful test assistant.",
        "options": {
          "systemMessage": "You are a helpful test assistant.",
          "maxIterations": 10
        }
      }
    },
    {
      "id": "openai_model",
      "name": "OpenAI Chat Model",
      "type": "n8n-nodes-langchain.lmChatOpenAi",
      "typeVersion": 1,
      "position": [
        300.0,
        150.0
      ],
      "parameters": {
        "model": "gpt-3.5-turbo",
        "options": {
          "temperature": 0.7,
          "maxTokens": 1000
        }
      }
    },
    {
      "id": "memory",
      "name": "Window Buffer Memory",
      "type": "n8n-nodes-langchain.memoryBufferWindow",
      "typeVersion": 1,
      "position": [
        300.0,
        300.0
      ],
      "parameters": {
        "sessionKey": "={{ $json.sessionId }}",
        "contextWindowLength": 5
      }
    }
  ],
  "connections": {
    "Chat Trigger": {
      "main": [
        [
          {
            "node": "AI Agent",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionMode": "sequential",
    "timezone": "UTC"
  },
  "tags": [
    "ai",
    "chat",
    "langchain",
    "openai"
  ]
}